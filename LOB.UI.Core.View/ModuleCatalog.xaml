<?xml version="1.0" encoding="utf-8" ?> 
<Modularity:ModuleCatalog
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:Modularity="clr-namespace:Microsoft.Practices.Prism.Modularity;assembly=Microsoft.Practices.Prism">
    
    <Modularity:ModuleInfo
        Ref="DaoModule.xap"
        ModuleName="DaoModule"
        ModuleType="LOB.Dao.Nhibernate.Module, LOB.Dao.Nhibernate, Version=1.0.0.0">
    </Modularity:ModuleInfo>

    <Modularity:ModuleInfo
        Ref="BusinessModule.xap"
        ModuleName="BusinessModule"
        ModuleType="LOB.Business.Module, LOB.Business, Version=1.0.0.0">
        <Modularity:ModuleInfo.DependsOn>
            <sys:String>DaoModule</sys:String>
        </Modularity:ModuleInfo.DependsOn>
    </Modularity:ModuleInfo>
    
    <Modularity:ModuleInfo
        Ref="UICoreModule.xap"
        ModuleName="UICoreModule"
        ModuleType="LOB.UI.Core, LOB.UI.Core, Version=1.0.0.0">
        <Modularity:ModuleInfo.DependsOn>
            <sys:String>DaoModule</sys:String>
            <sys:String>BusinessModule</sys:String>
        </Modularity:ModuleInfo.DependsOn>
    </Modularity:ModuleInfo>
</Modularity:ModuleCatalog>